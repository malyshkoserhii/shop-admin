{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": ["./src/types"],
    "noImplicitReturns": false,
    "strict": true,
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"resolveJsonModule": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "node_modules",
		"babel.config.js",
  ]
}
